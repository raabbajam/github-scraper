{
  "name": "github-scraper",
  "version": "1.0.0",
  "description": "Github data scraper using api\\",
  "main": "index.js",
  "scripts": {
    "test": "DEBUG=raabbajam:* CONFIG=test mocha -w tests/*Spec.js tests/**/*Spec.js",
    "start": "DEBUG=raabbajam:* node index.js",
    "filter": "DEBUG=raabbajam:* node filter.js",
    "scraper": "DEBUG=raabbajam:* node scraper.js",
    "formatter": "DEBUG=raabbajam:* node formatter.js",
    "restarter": "DEBUG=raabbajam:* node restart.js",
    "pm2:sampler": "DEBUG=raabbajam:* pm2 start index.js --no-autorestart -c '*/30 * * * *'",
    "pm2:filter": "DEBUG=raabbajam:* pm2 start filter.js --no-autorestart -c '*/30 * * * *'",
    "pm2:scraper": "DEBUG=raabbajam:* pm2 start scraper.js --no-autorestart -c '*/30 * * * *'",
    "log:filter": "DEBUG=raabbajam:* pm2 logs filter",
    "s:all": "sshp -l root -f ./servers.txt",
    "all:filter": "sshp -l root -f ./servers.txt 'pm2 reload filter'"
  },
  "author": "Raabb Ajam",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^2.9.24",
    "chai": "^2.2.0",
    "cheerio": "^0.19.0",
    "debug": "^2.1.3",
    "event-stream": "^3.3.0",
    "faker": "^2.1.2",
    "github": "git://github.com/danielmiller-nl/node-github",
    "googleapis": "^2.0.2",
    "highland": "^2.4.0",
    "json-csv": "^1.0.5",
    "kue": "^0.8.12",
    "lodash": "^3.6.0",
    "moment": "^2.10.2",
    "nohm": "^0.9.6",
    "readable-stream": "^1.0.33",
    "redis": "^0.12.1",
    "request": "^2.55.0"
  }
}
